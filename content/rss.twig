<rss xmlns:content="http://purl.org/rss/1.0/modules/content/" xmlns:wfw="http://wellformedweb.org/CommentAPI/" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:sy="http://purl.org/rss/1.0/modules/syndication/" xmlns:slash="http://purl.org/rss/1.0/modules/slash/" version="2.0">
    <channel>
    <title>{{ title }}</title>
    <atom:link href="{{ baseURL }}?feed.rss" rel="self" type="application/rss+xml" />
    <link>{{ baseURL }}</link>
    <description>{{ siteDescription }}</description>
{% for page in pages %}
{% if page['date'] %}
    <item>
        <title>{{ page['title'] }}</title>
        <description><![CDATA[{{ page['description'] }}]]></description>
        <link>{{ page['url'] }}</link>
        <pubDate>{{ page['time']|date("D, d M Y H:i:s O") }}</pubDate>
        <guid>{{ page['url'] }}</guid>
        <content:encoded><![CDATA[{{ page['content'] }}]]></content:encoded>
    </item>
{% endif %}
{% endfor %}
    </channel>
</rss>
